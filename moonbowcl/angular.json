                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "port": 4200,
                        "host": "localhost",
                        "browserTarget": "moonbowcl:build",
                        "proxyConfig": "buildfiles/proxy.conf.js"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "moonbowcl:build:production"
                        },
                        "local": {
                            "browserTarget": "moonbowcl:build:local"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "moonbowcl:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
